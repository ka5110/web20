<navigation-rule>
    <from-view-id>/signup.xhtml</from-view-id>
    <navigation-case>
        <from-action>#{authorize.register}</from-action>
        <from-outcome>REGISTER_SUCCESS</from-outcome>
        <to-view-id>/index.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
        <from-action>#{authorize.register}</from-action>
        <from-outcome>REGISTER_FAILURE</from-outcome>
        <to-view-id>/index.xhtml</to-view-id>
    </navigation-case>
</navigation-rule>